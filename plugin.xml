<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            name="UApp"
            class="org.pky.uml.Perspective"
            id="uml.perspective">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="org.pky.uml.editor.UMLEditor"
            default="true"
            contributorClass="org.pky.uml.rcp.action.UAppActionBarContributor"
            id="org.pky.uml.editor.UMLEditor"
            name="%editor.name">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="org.pky.uml.browser.ModelBrowser"
            id="org.pky.uml.ModelBrowser"
            name="%view.name"
            restorable="true">
      </view>
      <view
            class="org.pky.uml.browser.PropertyBrowser"
            id="org.pky.uml.browser.PropertyBrowser"
            name="%view.name.0"
            restorable="true">
      </view>
      <view
            class="org.pky.uml.browser.ActionBrowser"
            id="org.pky.uml.browser.ActionBrowser"
            name="%view.name.1"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               id="org.pky.uml.menus.android"
               label="%menu.label"
               sibling_order="&lt;command&#x0A;                  commandId=&quot;uml.command1&quot;&#x0A;                  style=&quot;push&quot;&gt;&#x0A;            &lt;/command&gt;">
            <command
                  commandId="org.pky.uml.rcp.action.NewProjectAction"
                  label="%command.label"
                  style="push">
            </command>
            <command
                  commandId="org.pky.uml.rcp.action.ProjectLoadAction"
                  label="%command.label.0"
                  style="push">
            </command>
            <command
                  commandId="org.pky.uml.rcp.action.SaveProjectAction"
                  label="프로젝트 저장하기"
                  style="push">
            </command>
            <separator
                  name="uml.separator1"
                  visible="true">
            </separator>
            <menu
                  label="%menu.label.0">
               <command
                     commandId="org.pky.uml.rcp.action.AndroidDevicesAction"
                     label="%command.label.1"
                     style="push">
               </command>
               <command
                     commandId="org.pky.uml.rcp.action.LayoutGeneratorAction"
                     label="%command.label.2"
                     style="push">
               </command>
            </menu>
            <menu
                  label="%menu.label.1">
               <command
                     commandId="org.pky.uml.rcp.action.IOSGeneratorAction"
                     label="%command.label.3"
                     style="push">
               </command>
               <separator
                     name="uml.separator1">
               </separator>
               <command
                     commandId="org.pky.uml.rcp.action.DeviceTitleBarDesignAction"
                     label="네비게이션 디자인"
                     style="push">
               </command>
            </menu>
         </menu>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="org.pky.uml.rcp.action.LayoutGeneratorAction"
            name="%command.name">
      </command>
      <command
            id="org.pky.uml.rcp.action.NewProjectAction"
            name="%command.name.0">
      </command>
      <command
            id="org.pky.uml.rcp.action.AndroidDevicesAction"
            name="%command.name.1">
      </command>
      <command
            id="org.pky.uml.rcp.action.ProjectLoadAction"
            name="%command.name.2">
      </command>
      <command
            id="org.pky.uml.rcp.action.IOSGeneratorAction"
            name="IOSGeneratorAction">
      </command>
      <command
            id="org.pky.uml.rcp.action.SaveProjectAction"
            name="SaveProjectAction">
      </command>
      <command
            id="org.pky.uml.rcp.action.DeviceTitleBarDesignAction"
            name="DeviceTitleBarDesignAction">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.pky.uml.rcp.action.AndroidGeneratorAction"
            commandId="org.pky.uml.rcp.action.LayoutGeneratorAction">
      </handler>
      <handler
            class="org.pky.uml.rcp.action.NewProjectAction"
            commandId="org.pky.uml.rcp.action.NewProjectAction">
      </handler>
      <handler
            class="org.pky.uml.rcp.action.AndroidDevicesAction"
            commandId="org.pky.uml.rcp.action.AndroidDevicesAction">
      </handler>
      <handler
            class="org.pky.uml.rcp.action.ProjectLoadAction"
            commandId="org.pky.uml.rcp.action.ProjectLoadAction">
      </handler>
      <handler
            class="org.pky.uml.rcp.action.IOSGeneratorAction"
            commandId="org.pky.uml.rcp.action.IOSGeneratorAction">
      </handler>
      <handler
            class="org.pky.uml.rcp.action.SaveProjectAction"
            commandId="org.pky.uml.rcp.action.SaveProjectAction">
      </handler>
      <handler
            class="org.pky.uml.rcp.action.DeviceTitleBarDesignAction"
            commandId="org.pky.uml.rcp.action.DeviceTitleBarDesignAction">
      </handler>
   </extension>
   



</plugin>
